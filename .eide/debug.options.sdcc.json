{
    "version": 2,
    "beforeBuildTasks": [],
    "afterBuildTasks": [
        {
            "name": "clean",
            "command": "cd \"${OutDir}\" && del *.sym *.cdb *.adb *._*"
        }
    ],
    "global": {
        "device": "mcs51",
        "optimize-type": "speed",
        "use-non-free": false
    },
    "c/cpp-compiler": {
        "language-c": "c99"
    },
    "asm-compiler": {},
    "linker": {
        "$mainFileName": "main",
        "executable-format": "hex",
        "output-lib": false
    }
}